{
  "name": "ellie",
  "description": "ellie-app.com",
  "repository": "https://github.com/lukewestby/ellie.git",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "6.9.5"
  },
  "dependencies": {
    "ajv": "5.2.2",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-remove-code": "0.0.6",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2017": "6.24.1",
    "codemirror": "5.27.4",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "elm": "0.18.0",
    "elm-css-webpack-loader": "https://github.com/OzTK/elm-css-webpack-loader.git#6194f2be3f99e0a9d521c3cabf87b2898cb4fa6b",
    "elm-format": "0.7.0-exp",
    "elm-test": "0.18.7",
    "elm-webpack-loader": "4.3.1",
    "es6-promise": "4.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "json-loader": "0.5.4",
    "postcss-loader": "2.0.6",
    "string-replace-webpack-plugin": "0.1.3",
    "style-loader": "0.18.2",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.1",
    "webpack-manifest-plugin": "1.1.2",
    "webpack-md5-hash": "0.0.5"
  },
  "scripts": {
    "watch": "webpack-dev-server --watch --config ./webpack.dev.config.js",
    "clean": "rm -rf ./build",
    "install:elm": "elm-package install --yes",
    "clean:elm": "rm -rf ./elm-stuff/build-artifacts/0.18.0/user",
    "build:make:0.18.0": "NODE_ENV=production cd ./make/0.18.0 && npm install && npm run build",
    "build:make": "npm run build:make:0.18.0",
    "build:production": "NODE_ENV=production npm run install:elm && npm run clean && webpack --config ./webpack.prod.config.js",
    "deploy": "python ./scripts/deploy_assets.py",
    "heroku-postbuild": "npm test && npm run build:make && npm run build:production && npm run deploy",
    "test:make:0.18.0": "cd ./make/0.18.0 && npm install && npm test",
    "test:make": "npm run test:make:0.18.0",
    "test": "npm run test:make"
  }
}
