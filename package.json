  "scripts": {
    "watch": "webpack-dev-server --watch --config ./webpack.dev.config.js",
    "clean": "rimraf ./build",
    "install:elm": "elm-package install --yes",
    "clean:elm": "rimraf ./elm-stuff/build-artifacts/0.18.0/user",
    "build:dll": "NODE_ENV=production webpack --config ./webpack.dll.config.js",
    "build:editor:production": "npm run clean:elm && BUILD_TARGET=editor webpack --config ./webpack.prod.config.js",
    "build:embed:production": "npm run clean:elm && BUILD_TARGET=embed webpack --config ./webpack.prod.config.js",
    "build:production": "npm run install:elm && npm run clean && npm run build:editor:production && npm run build:embed:production",
    "deploy": "python ./scripts/deploy_assets.py",
    "heroku-postbuild": "npm run build:production && npm run deploy"
  }
